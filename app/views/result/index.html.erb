<div class="container">
  <h2>Characters</h2>
  <% if @characters %>
    <% @characters.each do |chr| %>
      <p><%= chr.name %></p>
    <% end %>
  <% end %>
  <h2>Comics</h2>
  <% if @comics %>
    <% @comics.each do |cmc| %>
      <p><%= cmc.name %></p>
    <% end %>
  <% end %>
  <h2>Series</h2>
  <% if @series %>
    <% @series.each do |srs| %>
      <p><%= srs.name %></p>
    <% end %>
  <% end %>
  <h2>Creators</h2>
  <% if @creators %>
    <% @creators.each do |crt| %>
      <p><%= crt.fullName %></p>
    <% end %>
  <% end %>
</div>
